#!/usr/bin/env python

import fuzzymatch, sys

def usage():
	print "Usage:\n\nfuzzymatch source_table.csv merge_table.csv result.csv\n" 
	exit(-1)

if __name__ == "__main__":
	if len(sys.argv) < 4:
		print "wrong argument count"
		usage()
 
	from_file = sys.argv[1]
	to_file = sys.argv[2]
	out_file = sys.argv[3]

	fuzzymatch.run(from_file, to_file, out_file)
